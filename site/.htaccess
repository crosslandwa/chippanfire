# correct if missing .html or ends with trailing /
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule ^([^/.]+)/?$ http://chippanfire.com/$1.html  [R=301,L]
</IfModule>

# software 301s
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule ^software/([^/]+)/?$ http://chippanfire.com/software-$1.html  [R=301,L]
</IfModule>

ErrorDocument 404 http://chippanfire.com/error.html
